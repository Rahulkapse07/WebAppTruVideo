<suite name="RC Smoke Suite" configfailurepolicy="continue" parallel="false" thread-count="3"
	verbose="5" preserve-order="false">
	<listeners>
		<listener class-name="com.truvideo.testutils.Listeners"></listener>
	</listeners>
	<parameter name="baseUrl"
		value="https://rc.truvideo.com/login" />
	<parameter name="browser" value="chrome" />
	<parameter name="headless" value="false" />

	<test name="RC smoke test">
		<classes>
			<class name="com.truvideo.tests.LoginPageTest"></class>
			<class name="com.truvideo.tests.RepairOrderDetailPageTest" />
		</classes>
	</test>
	<test name="Message order service">
		<classes>
			<class name="com.truvideo.tests.Message_RepairOrdertest">
			</class>
		</classes>
	</test>



</suite>
